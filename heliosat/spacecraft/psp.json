{
    "spacecraft": {
        "psp": {
            "body_name": "SPP",
            "class_name": "PSP",
            "kernel_group": "psp",
            "keys": {
                "psp_fields_l2": {
                    "alt_keys": ["mag"],
                    "base_url": "$https://spdf.gsfc.nasa.gov/pub/data/psp/fields/l2/mag_rtn_1min/{YYYY}/psp_fld_l2_mag_rtn_1min_{YYYY}{MM}{DD}_v([0-9]{2}).cdf",
                    "versions": {
                        "01": {
                            "data_columns": [
                                {
                                    "key": "psp_fld_l2_mag_RTN_1min",
                                    "load_by_default": true,
                                    "names": ["BRTN", "B_RTN"],
                                    "reference_frame": "SPP_RTN"
                                }
                            ],
                            "time_column": {
                                "key": "epoch_mag_RTN_1min"
                            }
                        }
                    },
                    "format": "nasa_cdf"
                },
                "psp_spc_l3": {
                    "alt_keys": ["proton"],
                    "base_url": "https://spdf.gsfc.nasa.gov/pub/data/psp/sweap/spc/l3/l3i/{YYYY}/psp_swp_spc_l3i_{YYYY}{MM}{DD}_v([0-9]{2}).cdf",
                    "versions": {
                        "01": {
                            "data_columns": [
                                {
                                    "key": "np_moment",
                                    "load_by_default": true,
                                    "names": ["density"],
                                    "valid_range": [-1e6, 1e6]
                                },
                                {
                                    "key": "vp_moment_RTN",
                                    "load_by_default": true,
                                    "names": ["velocity"],
                                    "reference_frame": "SPP_RTN",
                                    "valid_range": [-1e6, 1e6]
                                },
                                {
                                    "key": "wp_moment",
                                    "load_by_default": true,
                                    "names": ["temperature"],
                                    "valid_range": [-1e6, 1e9]
                                }
                            ],
                            "time_column": {
                                "key": "Epoch"
                            }
                        }
                    },
                    "format": "nasa_cdf"
                }
            }
        }
    },
